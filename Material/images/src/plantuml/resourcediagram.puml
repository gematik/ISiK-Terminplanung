namespace ISiK_Terminplanung {

   class ISiKKalender <Schedule><<(P,#8DA5C4)>>{
      extension : **ScheduleName**
      |_ (All Slices)
         |_ <s> extension </s>
         |_ KalenderName : **Extension**
      active
      serviceType
